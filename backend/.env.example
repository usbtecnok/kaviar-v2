# Environment
NODE_ENV=development
PORT=3001

# Database - Supabase PostgreSQL
# Format: postgresql://postgres:<DATABASE_PASSWORD>@db.<PROJECT_ID>.supabase.co:5432/postgres
# Get DATABASE_PASSWORD from: Supabase Dashboard > Settings > Database > Database password
# Get PROJECT_ID from: Supabase Dashboard URL or Settings > General
DATABASE_URL="postgresql://postgres:YOUR_DATABASE_PASSWORD@db.YOUR_PROJECT_ID.supabase.co:5432/postgres"

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h

# Admin
ADMIN_DEFAULT_EMAIL=admin@kaviar.com
ADMIN_DEFAULT_PASSWORD=admin123
